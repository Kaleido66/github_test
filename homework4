'循环
l = [1,2,3,4,5]
size=len(l)
def for_way(l):
    for i in range(size-1,-1,-1):
        print(l[i],end=" ")

def whway(l):
    size=5
    while size>0:
        print(l[size-1],end=" ")
        size=size-1

for_way(l)
print("\n")
whway(l)
